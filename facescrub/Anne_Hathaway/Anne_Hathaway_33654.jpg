			<script type="text/javascript">
	document.cookie = "resolution=" + Math.max(screen.width, screen.height) + "; path=/";
	document.cookie='resolution='+Math.max(screen.width,screen.height) + '; path=/';
	
	var facebookAppId = '420471148000021';
	var googleClientId = '436083542406-2qdog56to22e89qe99n23dku1pljrktl.apps.googleusercontent.com';
	var site_url = "http://www.missmalini.com";
	var site_directory_url = "http://www.missmalini.com/wp-content/themes/missmalini";
	var postId = 0;
	var postPermalink = '';
	var is_single = 0;
	var is_home = 0;
	var is_archive = 0;
	var is_category = 0;
	var is_tag = 0;
	var is_search = 0;
	var is_photos_videos = 0;
	var in_motion = false;
	
	// for google api
	window.___gcfg = {
		lang: 'en-US',
		parsetags: 'onload'
	};
	var navMenuState = 0;
	
	function loadNavMenu(navMenu)
	{
		var categorySlug = navMenu.attr('data-slug');
		var categoryContainer = $('#'+categorySlug+' div.'+categorySlug+':first');
		
		if(categoryContainer.html() && categoryContainer.html().trim() == '')
		{
			categoryContainer.html('<div class="header_nav_loader" style="text-align:center;height:'+categoryContainer.prev().outerHeight()+'px;"><img src="'+site_directory_url+'/img/ajax-loader.gif" /></div>');
			
			$.ajax({
				url: site_url+"/api/common/get_megamenu_posts/?category_id="+categorySlug,
				cache:false,
				success: function (response) {
					var markup = '';
					
					for(var index in response.data)
					{
						console.log(index);
						markup += '<a href="'+response.data[index].post_permalink+'"><div class="col-lg-3 col-md-3 col-sm-3 col-xs-12 padding-removal-column border-sub-menu-img padding-bottom-15"><div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 padding-top-bottom-15" style="overflow:hidden;max-height:150px;"><img class="width-100" src="'+response.data[index].post_image+'"></div><div class="col-lg-12 col-md-12 col-sm-12 col-xs-12 sub-nav-ind-panel">'+response.data[index].post_title+'</div></div></a>';
					}
					
					categoryContainer.html(markup);
				}
			});
		}
	}
	
	function openNavMenu(navMenu)
	{
		var categorySlug = navMenu.attr('data-slug');
		$("#header_nav_tabs").finish();
		if(!navMenu.hasClass('sub-nav-li') && in_motion == false)
		{
			in_motion = true;
			navMenu.addClass('active');
			$('.mega-menus-wrap').css('display','block');
			$('#header_nav .tab-content#'+categorySlug).css('display','block');
			$("#header_nav_tabs").transition({y:'1px',opacity:1,easing:'snap',duration:150},function()
			{
				in_motion = false;
				loadNavMenu(navMenu);
				navMenuState = 1;
			});
		}
	}
	
	function switchNavMenu(navMenu)
	{
		var categorySlug = navMenu.attr('data-slug');
		
		if(!navMenu.hasClass('sub-nav-li'))
		{
			$('#header_nav .nav-li').removeClass('active');
			navMenu.addClass('active');
			$('.mega-menus-wrap').css('display','block');
			$('#header_nav .tab-content').css('display','none');
			$('#header_nav .tab-content#'+categorySlug).css('display','block');
			loadNavMenu(navMenu);
		}
	}
	
	function closeNavMenu(navMenu)
	{
		in_motion = true;
		$("#header_nav_tabs").transition({y:'-100%',opacity:0,easing:'snap',duration:150},function(){
		in_motion = false;
		$('#header_nav .nav-li').removeClass('active');
		$('.mega-menus-wrap').css('display','none');	
		$('#header_nav .tab-content').css('display','none');		
		navMenuState = 0;
		$("#header_nav_tabs").finish();
		});
	}
	
	function img_resize(){
		var img_proportion = 0.74;
		var img_resize = $('.img-resize').innerWidth();
		var img_resize_cal =  img_resize * img_proportion;
		$('.img-resize').css('height',img_resize_cal);
	}
	
	$(function () {
		
		$('.sub-cat-mob').hide();
		$('.nav-li-small a').click(function(){
			$(this).parent().find('.sub-cat-mob').slideToggle('slow');
		});
			
		$(window).resize(function(){
			img_resize();
		});
		img_resize();
		
		initFacebookShare();
		initTwitterTweet();
		initGooglePlusShare();
		initEmailShare();
		
		$('#myNavmenu').on('hide.bs.offcanvas',function(){
			$("body").css('overflow','scroll');
		});
		
		$('#header_nav .border-sub-menu-img').hover(
			function () {
				$(this).addClass('post-widget-shadow');
			},
			function () {
				$(this).removeClass('post-widget-shadow');
			}
		);
		
		$('#header_nav .nav-li').mouseenter(function () {
			var navMenu = $(this);
			
			//is nav currently open
			if(navMenuState == 0)
			{
				timer = setTimeout(function () { openNavMenu(navMenu); }, 150);
			}
