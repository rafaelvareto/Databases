$(function(){
  
  $('.blackFriday').each(function(){
    var $this = $(this),
        // Get the initial width, so it's not static in js. Just incase it ever changes.
        defWidth = $this.outerWidth();
    
    $this.on('click', function(){
      
      if($this.is('.active')){
        $this.removeClass('active');
        $this.animate({width: defWidth}, 1000);
      } else {
        $this.addClass('active');
        $this.animate({width: '291px'}, 1000);
      }
    });
    
  });
  
});

