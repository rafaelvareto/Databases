$(function(){ 
    /* Hide form input values on zip-lookup focus*/ 
    $('.zip-lookup:text').each(function(){
        var txtval = $(this).val();
        $(this).focus(function(){
            if($(this).val() == txtval){
                $(this).val('')
            }
        });
        $(this).blur(function(){
            if($(this).val() == ''){
                $(this).val(txtval);
            }
        });
    });
    
    $('#zip-submit').on('click', function() {
        $('#zipCheck').submit();
    });
    $('#zipCheck').on('submit',function(event){
        event.preventDefault();
        var zipField = $('#zip-lookup');
        var error = $('.error-text');
        zipField.removeClass('error');
        var zipVal = $.trim(zipField.val());
        if(zipVal.length != 5){
            zipField.addClass('error');
            error.addClass('show');
            return false;
        }else if(!$.isNumeric(zipVal)){
            zipField.addClass('error');
            return false;
        }else{
            _gaq.push(['_setCustomVar',2,'ZipCode',zipVal]); 
            $.ajax({
                   url: '../../../includes/usdish.com/zip-check.php',
                    data: {zip: zipVal},
                    type: 'post',
                    dataType: 'json',
                    success: function(response){
                        if(response){
                            var state = response.state.toLowerCase();
                            state = state.split(" ").join("-");                            
                            var state_abbrev = response.state_abbrev.toLowerCase();
                            var city = response.name;
                            var group_id = response.group_id;
                            city = city.split(" ").join("-");
                            city = city.toLowerCase();
                            if (group_id === undefined || group_id === null) {
                                window.location.href = '/dishnetwork-'+state+'.html';
                            } else {    
                                window.location.href = '/'+state_abbrev+'-dishnetwork-'+city+'.html';
                            }
                        }
                        if(!response){
                            window.location.href = '/packages.html';
                        }
                    }
            });
            error.removeClass('show');
        }
    });
    
    
    
    // Duplicate scripts for USD - Banner Zip Finder - A/B Test
    $('#abZip:text').each(function(){
        var txtval = $(this).val();
        $(this).focus(function(){
            if($(this).val() == txtval){
                $(this).val('')
            }
        });
        $(this).blur(function(){
            if($(this).val() == ''){
                $(this).val(txtval);
            }
        });
    });
    
    $('#abSubmit').on('click', function(event) {
        event.preventDefault();
        $('#abZipForm').submit();
    });
    
    
    
    $('#abZipForm').on('submit',function(event){
    event.preventDefault();
    var zipField = $('#abZip');
    var productSelect = $('#service option:selected').val();
    var error = $('.ab-error-text');
    zipField.removeClass('error');
    var zipVal = $.trim(zipField.val());
    _gaq.push(['_trackEvent', 'ZipFinderTest', 'Submitted', 'Index']);
    if(productSelect == 'optTV' && zipVal.length != 5){
        zipField.addClass('error');
        error.addClass('show');
        return false;
    }else if(productSelect == 'optTV' && !$.isNumeric(zipVal)){
        zipField.addClass('error');
        return false;
    }else if (productSelect == 'optTV') {
        _gaq.push(['_setCustomVar',2,'ZipCode',zipVal]); 
        _gaq.push(['_setCustomVar',3,'Service','TV Service']);  
        $.ajax({
               url: '../../../includes/usdish.com/zip-check.php',
                data: {zip: zipVal},
                type: 'post',
                dataType: 'json',
                success: function(response){
                    if(response){
                        var state = response.state.toLowerCase();
                        state = state.split(" ").join("-");                            
                        var state_abbrev = response.state_abbrev.toLowerCase();
                        var city = response.name;
                        var group_id = response.group_id;
                        city = city.split(" ").join("-");
                        city = city.toLowerCase();
                        if (group_id === undefined || group_id === null) {
                            window.location.href = '/dishnetwork-'+state+'.html';
                        } else {    
                            window.location.href = '/'+state_abbrev+'-dishnetwork-'+city+'.html';
                        }
                    }
                    if(!response){
                        window.location.href = '/packages.html';
                    }
                }
        });
        error.removeClass('show');
        } else if(productSelect == 'optInternet') {
            if ($.isNumeric(zipVal) && zipVal.length == 5) {
                _gaq.push(['_setCustomVar',2,'ZipCode',zipVal]); 
            }
            _gaq.push(['_setCustomVar',3,'Service','Internet']);     
            window.location.href = '/internet.html';        

        } else if(productSelect == 'optBundles') {
                   
           if ($.isNumeric(zipVal) && zipVal.length == 5) {
                _gaq.push(['_setCustomVar',2,'ZipCode',zipVal]); 
            } 
            _gaq.push(['_setCustomVar',3,'Service','Bundles']);               
            window.location.href = '/bundles.html'; 
        } else {
            console.log('Uh oh');
            event.preventDefault();
        }
    });
    
    // End duplicate scripts for USD - Banner Zip Finder - A/B Test
    
    });


