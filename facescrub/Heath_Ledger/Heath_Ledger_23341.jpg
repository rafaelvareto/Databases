	<script>
		
		/* -- MENU VARIABLES ------------------------ */
		var show 		= false;
		var prev_id 	= '';
		var current 	= '';
		var _counter 	= 0;
		var _seconds 	= 0;
			
		function openMenu() {

			_seconds++;

			if(_seconds == 2) { 
				showMenu();
				clearInterval(_counter);
			}
		
		}
		
		function showMenu() {
							
			var id = '#'+ current.attr('rel');
			var li = current.parent();
			var pr = $('.menu-current');
				
			pr.removeClass('menu-current');
			li.addClass('menu-current');
				
			if (!show) {
				$(id).slideDown(500);
				show = true;
				prev_id = id;
			} else {
				$(prev_id).hide();
				$(id).show();
				prev_id = id;	
			}

		}

		$(document).ready(function() {
			
			// CHECK FOR ANALYTICS EVENT DETECTION --------------------------------------- //
			$("a[data-event]").click(function(e) {
				var ev = $(this).attr('data-event');
				ga('send', 'event', 'linksource', ev);
				return true;
			});
			
			if (screen.width <= 768) {
				
				$('.menu-link').click(function(e) { 
					current = $(this);
					var id 	= '#' + current.attr('rel');
					if (id != prev_id) {
						e.preventDefault();
						showMenu();
					}
				});

			} else {
			
				$(".menu-link").hover(function(){
					
					current = $(this);
					
					if (!show) {
						 _counter = setInterval(openMenu, 250);
					} else {
						showMenu()
					}
		
				}, function() {
					clearInterval(_counter);
				});
			
			}
			
			/* -- FACEBOOK RENDER FOR HIDDEN DIV ------- */	
			var subscribe	= document.getElementsByClassName("social-media-icon-h")[0];
			var fb_wrap		= document.getElementsByClassName("fb-wrap")[0]; 
    		var rendered 	= false;

			window.fbAsyncInit = function () {
    			subscribe.onmouseover = function () {
        			if (!rendered) {
            			FB.XFBML.parse(fb_wrap);
           				rendered = true;
        			}
    			};
			};
			
			$(".social-media-icon-h").hover(function(e) {
				var $em = $(this).find(".social-media-badge");
				$em.css("z-index", '101');
				$em.hide();
				$em.slideDown(250);
			}, function(e) {
				$(this).find(".social-media-badge").slideUp(250);
			});
						
		});
	</script>
