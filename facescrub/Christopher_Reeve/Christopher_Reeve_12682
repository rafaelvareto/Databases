  (function () {
    function clean(input) {
      while (input.lastIndexOf('/') === input.length - 1) {
        input = input.substr(0, input.length - 1)
      }
      return input;
    }
    var rootPath = clean("http:\/\/s.mirror.co.uk\/resources\/");
    tm.config.set("resources.rootPath", rootPath);
    tm.config.set("frontend.rootPath", rootPath + "/..");
    tm.config.set("resources.js.rootPath", rootPath + "/js");
    tm.config.set("resources.skin.name", "mirror");
    tm.config.set("resources.skin.url", clean("http:\/\/s.mirror.co.uk\/skins\/mirror\/"));
    tm.config.set("resources.skin.sharedUrl", clean("http:\/\/s.mirror.co.uk\/skins\/shared\/"));
    tm.config.set("resources.handlebars.rootPath", rootPath + "/../template/handlebars/widgets");
