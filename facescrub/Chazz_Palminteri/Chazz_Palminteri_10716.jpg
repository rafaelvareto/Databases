        var FlashSetImageProperties, __bind = function(fn, me){ return function(){ return fn.apply(me, arguments); }; };

        FlashSetImageProperties = (function() {

            function FlashSetImageProperties(artistType, artist, category, slide, photo)
            {
                this.artistType = artistType;
                this.artist = artist;
                this.category = category;
                this.slide = slide;
                this.photo = photo;
                this.callbackFunction = __bind(this.callbackFunction, this);
            }

            FlashSetImageProperties.prototype.callbackFunction = function(result) {
                return swfobject.getObjectById("liveBooks").fotomoto_getImageProducts(JSON.stringify(result), this.artistType, this.artist, this.category, this.slide, this.photo);
            };

            return FlashSetImageProperties;
        })();

        function fotomoto_getImageProducts(image_url, activeArtistType, activeArtist, activeCategory, new_slide, i)
        {
            window.setTimeout(function() {
                window.imageProperties = new FlashSetImageProperties(activeArtistType, activeArtist, activeCategory, new_slide, i);
                FOTOMOTO.API.getImageProducts(image_url, window.imageProperties.callbackFunction);
            }, 0);
            return 1;
        }
