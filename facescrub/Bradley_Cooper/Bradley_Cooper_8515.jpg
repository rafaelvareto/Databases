<script type='text/javascript'>
    var oop_slot = "adinterstitial",
        gatefold_slot = "gatefold-container",
        interstitial_desktop_slot = "adinterstitial-responsive";

    googletag.cmd.push(function() {
      googletag.pubads().addEventListener('slotRenderEnded', function(event) {
        var id = event.slot.b.d;
        if(id == oop_slot){
          var iframe = $($("#"+oop_slot).find("iframe")[0]).contents().find("body").html();
          $("body").append(iframe);
        }

        if (id == interstitial_desktop_slot) {
          var iframe = $($("#"+interstitial_desktop_slot).find("iframe")[0]).contents().find("body").html();
          $("body").append(iframe);
          $("#" + interstitial_desktop_slot).hide();
        }
      });
    });

