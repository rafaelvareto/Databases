  function getVisitorInfo() {
    var lre1=/\.facebook\.com$/gi;
    var splitted_url = document.referrer.split('/');
    var roiOptions = {
          localCookieName: 'roi_track',
          sourceParameter: 'source',
          keywordParameter: 'q',
          keywordTrackingParameter: 'qroi',
          remoteCookieDuration: 0
        },
        visitorInfo = {source: '', keyword: '', paid: false, justLanded: true},
        cookieDomain = document.domain.substring(document.domain.indexOf('.')),
//        campaignSource = blogoUtils.extractParamFromUri(window.location.href, roiOptions.sourceParameter),
        campaignSource = blogoUtils.extractParamFromUri(window.location.href, roiOptions.sourceParameter) || (lre1.test(splitted_url[2]) ? 'facebook' : null)
        campaignKeywords = campaignSource ? blogoUtils.extractParamFromUri(window.location.href, roiOptions.keywordParameter) : '',
        trackingKeywords = campaignSource ? blogoUtils.extractParamFromUri(window.location.href, roiOptions.keywordTrackingParameter) : '',
        localCookie = blogoUtils.cookie(roiOptions.localCookieName);
    if (campaignSource) {
      //console.log("Paid visitor");
      visitorInfo.source = campaignSource;
      visitorInfo.keyword = campaignKeywords;
      visitorInfo.keyword_tracking = trackingKeywords;
      visitorInfo.paid = true;
    } else {
      // check cookie
      if (localCookie) {
        try {
            var cookieInfo = window.JSON.parse(localCookie);
            visitorInfo.source = cookieInfo.source || '';
            visitorInfo.keyword = cookieInfo.keyword || '';
            visitorInfo.keyword_tracking = cookieInfo.keyword_tracking || '';
            visitorInfo.paid = cookieInfo.paid || false;
        } catch (e) {
            blogoUtils.cookie(roiOptions.localCookieName, null, {path:'/', domain: cookieDomain});
        }
      }
    }
    if (!localCookie) {
        blogoUtils.cookie(roiOptions.localCookieName, window.JSON.stringify(visitorInfo),{path:'/', domain: cookieDomain});
    } else {
      //console.log(localCookie);
        visitorInfo.justLanded = false;
    }
    return visitorInfo;
  }

function TrackTiming(_tt) {
    
    var pub = this;
    pub.MAX_TIME = 10 * 60 * 1000;
    
    pub.disabled = false;
    pub.category = _tt.category;
    pub.variable = _tt.variable;
    pub.method = _tt.method ? _tt.method : 'gaOld._trackTiming';
    pub.label = _tt.label ? _tt.label : undefined;
    pub.startTime;
    pub.elapsedTime  = _tt.elapsedTime ? _tt.elapsedTime : undefined;
    pub.isDebug = false;
    
    if (window.blogoUtils && window.blogoUtils.cookie) {
        pub.isDebug = (window.blogoUtils.cookie('debugTrackTiming')) ? true : false;
    }

    pub.startTracking = function() {
        pub.startTime = new Date().getTime();
        return pub;
    };

    pub.endTracking = function() {
        pub.elapsedTime = new Date().getTime() - pub.startTime;
        return pub;
    };

    pub.debug = function(opt_enable) {
        pub.isDebug = (typeof opt_enable == "undefined") ? true : opt_enable;
        return pub;
    };

    pub.setCallback = function(_callback) {
        pub.userCallback = _callback;
        return pub;
    }

    pub.loadCallback = function() {
        pub.endTracking().send();
        if (pub.userCallback)
            pub.userCallback();
        return pub;
    }

    pub.send = function() {
      if (window._gaq &&
              pub.method &&
              pub.category &&
              pub.variable &&
              !pub.disabled) {
          if (0 < pub.elapsedTime && pub.elapsedTime < pub.MAX_TIME) {

              var command = [pub.method, pub.category, pub.variable,
                             pub.elapsedTime, pub.label];

              if (pub.isDebug) {
                // Override sample rate if debug is enabled.
                command.push(100);

                if (window.console && window.console.log) {
                   console.log(command);
                }
              }
              window._gaq.push(command);
          }
       }
       return pub;
    };
    
    return pub;
}



