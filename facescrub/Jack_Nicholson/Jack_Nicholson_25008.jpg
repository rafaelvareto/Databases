					var cat = path[4];
				} else {
					var cat = path[3];
				}
				var isc = cat + " scrolled - " + scroll_count ;
				_gaq.push(["_trackEvent", "Infinite Scroll", isc, url_to_track]);
				trackInfiniteScrollClick(cat);
				scroll_count++;

				if (typeof nr_domain === 'undefined'){
					jQuery("#is-nrelate").remove();
				} else {
					window.nRelate.trigger();
				}
			}
