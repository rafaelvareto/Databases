<script type="text/javascript">	
$(function(){
	 
	 var caret = "<span class='caret'></span>";
	 var sisterToggle = $('.sister-toggle .dropdown-toggle');
	 
	 function brotherSister(activeText, elements, sameElement) {
		  // append the active sister elements text to the sister toggle 
		  sisterToggle.text('Filter by: ' + activeText);
		  // append the caret symbol to the dropdown toggle
		  sisterToggle.append(' ' + caret);
		  // remove all active classes from brother elements
		  elements.removeClass('active');
		  // add active to the brother element with the same text as the sister element
		  sameElement.addClass('active');
	 }

	 $('.sister-toggle ul li').click( function(){
				// SISTER TOGGLE
		  var activeText = $('.sister-toggle .active').text(), // get text from sister active element
				// BROTHER LIST
				brotherElements = $('.brother-list ul li'), // targets the brother element list
				brotherSameElement = $( '.brother-list ul li:contains("' + activeText + '") a' ); // targets the  brother element with the same text as the sister active element

		  brotherSister(activeText, brotherElements, brotherSameElement);
	 });

	 $('.brother-list ul li a').click( function(){ 
				// BROTHER LIST
		  var activeText = $('.brother-list .active').text(), // get text from brother active element
				// SISTER TOGGLE
				sisterElements = $('.sister-toggle ul li'), // targets the brother element list
				sisterSameElement = $( '.sister-toggle ul li:contains("' + activeText + '") a' ); // targets the  brother element with the same text as the sister active element
				
		  brotherSister(activeText, sisterElements, sisterSameElement);
	 });
});
</script>

		  
